<tr class="bath guest-no-<%= guest.id %>" data-href="<%= edit_bath_path(guest.bath.id ) %>" >
  <th scope="col"></th>
  <th scope="col"><%= "#{guest.first_name} #{guest.last_name}" %></th>
  <th scope="col"><%= guest.bath.remark_bath %></th>
  <th scope="col"><%= guest.bath.timing.name %></th>
  <th scope="col"><%= guest.bath.infection_id == 1 ? "" : "#{guest.bath.infection.name}" %></th>
</tr>